<snj>
<set xpath="//window_group[@name='workstation_campfire']">
			<window name="windowCraftingListCampfire"/>
			<window name="craftingInfoPanel"/>
			<window name="windowToolsCampfire" />
			<window name="windowFuelPropane" />
			<window name="windowCampOutput" />
			<window name="windowCraftingQueueCamp"/>
			<window name="windowNonPagingHeader" />
	</set>

<set xpath="//window_group[@name='workstation_forge']">
			<window name="windowCraftingListForge"/>
			<window name="craftingInfoPanel"/>
			<window name="windowForgeInput" />
			<window name="windowToolsForge" />
			<window name="windowOutputForge" />
			<window name="windowCraftingQueueCamp"/>
			<window name="windowFuelElectricv2" />
			<window name="windowNonPagingHeader" />
			
	</set>	


<set xpath="//window_group[@name='workstation_workbench']">
			<window name="windowCraftingListWorkbench"/>
			<window name="craftingInfoPanel"/>
			<window name="windowOutputWorkbench" />
			<window name="windowCraftingQueueCamp"/>
			<window name="windowToolsBench"/>	
			<window name="windowFuelElectric" />
			<window name="windowNonPagingHeader" />
	</set>
	<set xpath="//window_group[@name='workstation_cementMixer']">
			<window name="windowCraftingListMixer"/>
			<window name="craftingInfoPanel"/>
			<window name="windowOutputWorkbench" />
			<window name="windowCraftingQueueCamp"/>
			<window name="windowToolsMixer" />
			<window name="windowFuelElectric" />
			<window name="windowNonPagingHeader" />
			
	</set>
	<set xpath="//window_group[@name='workstation_chemistryStation']">
			<window name="windowCraftingListChemistry"/>
			<window name="craftingInfoPanel"/>
			<window name="windowFuel" />
			<window name="windowOutputForge" />
			<window name="windowCraftingQueueCamp"/>
			<window name="windowNonPagingHeader" />
			<window name="windowToolsStation"/>
	</set>

<append xpath="/xui/ruleset">
	
			
		<window_group name="workstation_HOH_Collection_workstations_Range" controller="XUiC_WorkstationWindowGroup" open_backpack_on_open="true" close_compass_on_open="true">
				<window name="windowCraftingListCampfire"/>
			<window name="craftingInfoPanel"/>
			<window name="windowCraftingQueueCamp"/>
			<window name="windowToolsCoffeeMaker" />
			<window name="windowFuelElectric" />
			<window name="windowCampOutput" />
			<window name="windowNonPagingHeader" />
		</window_group>
        <window_group name="workstation_HOH_Collection_workstations_CoffeeMaker" controller="XUiC_WorkstationWindowGroup" open_backpack_on_open="true" close_compass_on_open="true">
			<window name="windowCraftingListCampfire"/>
			<window name="craftingInfoPanel"/>
			<window name="windowCraftingQueueCamp"/>
			<window name="windowToolsCoffeeMaker" />
			<window name="windowFuelElectric" />
			<window name="windowCampOutput" />
			<window name="windowNonPagingHeader" />
		</window_group>
		
		<window_group name="workstation_WorkingStove" controller="XUiC_WorkstationWindowGroup" open_backpack_on_open="true" close_compass_on_open="true" defaultSelected="bp.content">
			<window name="windowCraftingListCampfire"/>
			<window name="craftingInfoPanel"/>
			<window name="windowToolsStove" />
			<window name="windowFuelElectric" />
			<window name="windowCampOutput" />
			<window name="windowCraftingQueueCamp"/>
			<window name="windowNonPagingHeader" />
		</window_group>
		<window_group name="workstation_WorkingGasGrill" controller="XUiC_WorkstationWindowGroup" open_backpack_on_open="true" close_compass_on_open="true" defaultSelected="bp.content">
			<window name="windowCraftingListCampfire"/>
			<window name="craftingInfoPanel"/>
			<window name="windowToolsGrill" />
			<window name="windowCampFuel" />
			<window name="windowCampOutput" />
			<window name="windowCraftingQueueCamp"/>
			<window name="windowNonPagingHeader" />
		</window_group>
		<window_group name="workstation_WorkingCharcoalGrill" controller="XUiC_WorkstationWindowGroup" open_backpack_on_open="true" close_compass_on_open="true" defaultSelected="bp.content">
			<window name="windowCraftingListCampfire"/>
			<window name="craftingInfoPanel"/>
			<window name="windowToolsGrill" />
			<window name="windowCampFuel" />
			<window name="windowCampOutput" />
			<window name="windowCraftingQueueCamp"/>
			<window name="windowNonPagingHeader" />
		</window_group>
		
				
	</append>




</snj>