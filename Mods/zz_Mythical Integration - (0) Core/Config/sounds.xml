<MythicalIntegration>
<!-- This allows sounds to be added to integration -->

	<append xpath="/Sounds">
		<SoundDataNode name="myth_sound_omae_wa">
			<AudioSource name="Sounds/AudioSource_UI"/>
			<AudioClip ClipName="#@modfolder:Assets/dodgethis.unity3d?sounds/omae_wa_mo_shindeiru.mp3"/>
			<LowestPitch name="0.9"/>
			<HighestPitch name="1.0"/>
			<LocalCrouchVolumeScale value="1"/> <CrouchNoiseScale value="1"/> <NoiseScale value="1"/> <MaxVoices value="30"/> <MaxVoicesPerEntity value="10"/> <MaxRepeatRate value="0.001"/> 		</SoundDataNode>
		
		
		<SoundDataNode name="myth_sound_nani">
			<AudioSource name="Sounds/AudioSource_UI"/>
			<AudioClip ClipName="#@modfolder:Assets/dodgethis.unity3d?sounds/nani_anime.mp3"/>
			<LowestPitch name="0.9"/>
			<HighestPitch name="1.0"/>
			<LocalCrouchVolumeScale value="1"/> <CrouchNoiseScale value="1"/> <NoiseScale value="1"/> <MaxVoices value="30"/> <MaxVoicesPerEntity value="10"/> <MaxRepeatRate value="0.001"/> 
		</SoundDataNode>
	</append>
</MythicalIntegration>