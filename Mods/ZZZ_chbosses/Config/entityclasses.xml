			<drop event="Harvest" name="resourceFeather" tag="butcherHarvest" count="60"/>
			<drop event="Harvest" name="resourceBone" tag="butcherHarvest" count="15"/>
			<drop event="Harvest" name="resourceBone" tag="allToolsHarvest" count="7"/>
