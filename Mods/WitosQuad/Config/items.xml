<Witos>
	<append xpath="/items">
	<item name="vehicleWitosQuadPlaceable">
			<property name="Extends" value="vehicleMinibikePlaceable"/>
			<property name="CreativeMode" value="Player"/>
            <property name="DescriptionKey" value="vehicleWitosQuadPlaceableDesc"/>
            <property name="CustomIcon" value="Quad"/>
			<property name="Tags" value="vehicle,vengine,vfuel,vlight,vplow,vseat,canHaveCosmetic"/>
			<property name="Meshfile" value="#@modfolder:Resources/WitosQuad.unity3d?WitosQuad"/>
			<property name="EconomicValue" value="30000"/>
			<property class="Action1">
				<property name="Vehicle" value="vehicleWitosQuad"/>
				<property name="VehicleSize" value="4.5, 3.1, 7"/>
			</property>
			<effect_group name="main" tiered="false">
				<passive_effect name="ModSlots" operation="base_set" value="5"/>
				<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
				<passive_effect name="DegradationMax" operation="base_set" value="8000"/>
			</effect_group>
		</item>
		
		<item name="Quadchassis">
			<property name="Extends" value="vehicleWheels"/>
			
			<property name="CustomIcon" value="Quadchasis"/>
			<property name="DescriptionKey" value="vehicleQuadchassisDesc"/>
			<property name="EconomicValue" value="3000"/>
			<property name="TraderStageTemplate" value="vehicleTier2"/>
		</item>
		
		<item name="Quadbars">
			<property name="Extends" value="vehicleWheels"/>
			
			<property name="CustomIcon" value="Quadbars"/>
			<property name="DescriptionKey" value="vehicleQuadbarsDesc"/>
			<property name="EconomicValue" value="3000"/>
			<property name="TraderStageTemplate" value="vehicleTier2"/>
		</item>
		
	</append>
</Witos>