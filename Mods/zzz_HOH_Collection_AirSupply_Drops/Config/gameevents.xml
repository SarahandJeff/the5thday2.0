<configs>
	<append xpath="/gameevents">
        <action_sequence name="HOHSupplyDropAll">
            <property name="category" value="admin" />
            <property name="action_type" value="Game" />
            <property name="target_type" value="Block" />
            <property name="allow_while_dead" value="true" />

            <action class="PlaySound">
                <property name="sound" value="HOHAirDropRadio" />
                <property name="phase" value="1" />
            </action>
            <action class="Delay">
                <property name="time" value="2" />
                <property name="phase" value="2" />
            </action>
            <action class="BlockReplace">
                <property name="target_position" value="0, 0, 0" />
                <property name="block_to" value="HOHSupplyDropCall" />
                <property name="phase" value="3" />
            </action>
           <action class="Delay">
    			<property name="time" value="5" />
	    		<property name="phase" value="4" />
		    </action>
            <action class="PlaySound">
                <property name="sound" value="HOHAirDropPlane" />
                <property name="phase" value="5" />
            </action>
            <action class="BlockReplace">
                <property name="target_position" value="0, 0, 0" />
                <property name="block_to" value="C17AD0" />
                <property name="phase" value="5" />
            </action>
            <action class="Delay">
                <property name="time" value="10" />
                <property name="phase" value="9" />
            </action>
            <action class="BlockReplace">
                <property name="target_position" value="0, 0, 0" />
                <property name="block_to" value="C17AD1"/>
                <property name="phase" value="10" />
            </action>
            <action class="Delay">
                <property name="time" value="8" />
                <property name="phase" value="11" />
            </action>
            <action class="SpawnEntity">
                <property name="entity_names" value="zombieArleneFeral,zombieBoeFeral,zombieJoeFeral,zombieLabFeral,zombieDarleneFeral,zombieMarleneFeral,zombieYoFeral,zombieSteveFeral,zombieBusinessManFeral,zombieNurseFeral,zombieSkateboarderFeral,zombiePartyGirlFeral,zombieBurntFeral,zombieJanitorFeral"  />
                <property name="spawn_count" value="3-7" param1="spawncount" />
                <property name="party_addition" value="2" param1="partyaddition" />
                <property name="min_distance" value="10" />
                <property name="max_distance" value="20" />
			    <property name="safe_spawn" value="true" />
                <property name="phase" value="12" />
            </action>
        </action_sequence>


