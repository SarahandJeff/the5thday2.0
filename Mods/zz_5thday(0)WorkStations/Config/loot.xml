<snj>
<append xpath="/lootcontainers" >
		<lootcontainer name="ChickenCoop" size="2,1" count="2" sound_open="UseActions/open_chest" sound_close="UseActions/close_chest" destroy_on_close="true" loot_quality_template="qualBaseTemplate">
			<item name="foodEgg" count="1,5"/>
			<item name="foodRawChicken" count="1,3"/>
			<item name="resourceFeather" count="1,3"/>
			<item name="resourceChickenPoop" count="1,3"/>
		</lootcontainer>
		<lootcontainer name="Pigsty" size="2,1" count="2" sound_open="UseActions/open_chest" sound_close="UseActions/close_chest" destroy_on_close="true" loot_quality_template="qualBaseTemplate">
			<item name="foodRawPorkMeat" count="1,5"/>
			<item name="animalPiglet" count="1" prob=".015"/>
		</lootcontainer>

		<lootcontainer name="MilkingStation" size="2,1" count="2" sound_open="UseActions/open_chest" sound_close="UseActions/close_chest" destroy_on_close="true" loot_quality_template="qualBaseTemplate">
			<item name="foodRawMeat" count="1,5"/>
			<item name="foodMilk" count="1,5"/>
			<item name="animalCalf" count="1" prob=".015"/>
		</lootcontainer>




		
	</append>
