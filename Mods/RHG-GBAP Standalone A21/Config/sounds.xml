<GBAP>
<append xpath="/Sounds">

<SoundDataNode name="Auger_s_Fire"> 
	    <AudioSource name="Sounds/Weapons/Motorized/Auger/AugerAudioSource"/> 
	    <NetworkAudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab_Network"/>
		<Noise ID="6" noise="60" time="2" heat_map_strength="1.4" heat_map_time="180"/>
		<Sequence/>
		<AudioClip ClipName="Sounds/Weapons/Motorized/Auger/Auger_Fire_Start"/>
		<AudioClip ClipName="Sounds/Weapons/Motorized/Auger/auger_fire_lp" Loop="true"/>
		<AudioClip ClipName="Sounds/Weapons/Motorized/Auger/auger_fire_end"/>
		<LocalCrouchVolumeScale value="0.01"/> 
		<CrouchNoiseScale value="0.5"/> 
		<NoiseScale value="0.1"/> 
		<MaxVoices value="0"/> 
		<MaxRepeatRate value="0.001"/>
	</SoundDataNode>

	<SoundDataNode name="Chainsaw_s_Fire"> 
	    <AudioSource name="Sounds/Weapons/Motorized/Auger/AugerAudioSource"/> 
		<NetworkAudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab_Network"/>
		<Noise ID="6" noise="60" time="2" heat_map_strength="1.4" heat_map_time="180"/>
		<Sequence/>
		<AudioClip ClipName="Sounds/Weapons/Motorized/Chainsaw/chainsaw_fire_start"/>
		<AudioClip ClipName="Sounds/Weapons/Motorized/Chainsaw/chainsaw_fire_lp" Loop="true"/>
		<AudioClip ClipName="Sounds/Weapons/Motorized/Chainsaw/chainsaw_fire_end"/>
		<LocalCrouchVolumeScale value="0.01"/> 
		<CrouchNoiseScale value="0.5"/> 
		<NoiseScale value="0.1"/> 
		<MaxVoices value="0"/> 
		<MaxRepeatRate value="0.001"/>
	</SoundDataNode>

    <SoundDataNode name="MilwackDrillStart"> 
		 <AudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab"/> 
		 <NetworkAudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab_Network"/>
			<Noise ID="1" range="5" volume="30" time="2" heat_map_strength="0.2" heat_map_time="90"/>
			<AudioClip ClipName="#@modfolder:Resources/Audio.unity3d?drill1"/>
			<AudioClip ClipName="#@modfolder:Resources/Audio.unity3d?drill1"/>
			<LocalCrouchVolumeScale value="1"/> 
			<CrouchNoiseScale value="1"/>
			<NoiseScale value="3"/> 
			<MaxVoices value="5"/> 
			<MaxRepeatRate value="0.001"/>
	</SoundDataNode>

<SoundDataNode name="MilwackDrill_sil"> 
		 <AudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab"/> 
		 <NetworkAudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab_Network"/>
		<Noise ID="1"  range="5" volume="0" time="1" heat_map_strength="0.05" heat_map_time="10"/>
		<AudioClip ClipName="#@modfolder:Resources/Audio.unity3d?drill1"/>
		<LocalCrouchVolumeScale value="0.1"/> 
		<CrouchNoiseScale value="0.5"/>
		<NoiseScale value="0.2"/> 
		<MaxVoices value="0"/> 
		<MaxRepeatRate value="0.001"/>
	</SoundDataNode>
  
  <SoundDataNode name="MilwackDrillUpgrade"> 
		 <AudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab"/> 
		 <NetworkAudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab_Network"/>
			<Noise ID="1" range="5" volume="60" time="2" heat_map_strength="0.2" heat_map_time="90"/>
				<AudioClip ClipName="#@modfolder:Sounds/Sounds.unity3d?impactwrench" />
				<AudioClip ClipName="#@modfolder:Sounds/Sounds.unity3d?impactwrench1" />
				<AudioClip ClipName="#@modfolder:Sounds/Sounds.unity3d?impactwrench2" />
				<LocalCrouchVolumeScale value="1" /> 
				<CrouchNoiseScale value="1" /> 
				<NoiseScale value="1"/> 
				<MaxVoices value="10"/>  
				<MaxRepeatRate value="0.001" /> 
	</SoundDataNode>
  
  <SoundDataNode name="MilwackDrillUpgrade_sil"> 
		 <AudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab"/> 
		 <NetworkAudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab_Network"/>
			<Noise ID="1" range="5" volume="0" time="2" heat_map_strength="0.05" heat_map_time="10"/>
				<AudioClip ClipName="#@modfolder:Sounds/Sounds.unity3d?impactwrench" />
				<AudioClip ClipName="#@modfolder:Sounds/Sounds.unity3d?impactwrench1" />
				<AudioClip ClipName="#@modfolder:Sounds/Sounds.unity3d?impactwrench2" />
				<LocalCrouchVolumeScale value="0.1" /> 
				<CrouchNoiseScale value="0.1" /> 
				<NoiseScale value="0.1" /> 
				<MaxVoices value="0" /> 
				<MaxRepeatRate value="0.001" /> 
	</SoundDataNode>


</append>
</GBAP>














