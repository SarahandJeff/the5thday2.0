<append xpath="/progression/perks">

	<perk name="perkGroundBreakers" max_level="5" parent="skillStrengthConstruction" name_key="perkGroundBreakersName" desc_key="perkGroundBreakersDesc" icon="ui_game_symbol_resource">
		<level_requirements level="1"><requirement name="ProgressionLevel" progression_name="attStrength" operation="GTE" value="2" desc_key="reqStrengthLevel02"/></level_requirements>
		<level_requirements level="2"><requirement name="ProgressionLevel" progression_name="attStrength" operation="GTE" value="4" desc_key="reqStrengthLevel04"/></level_requirements>
		<level_requirements level="3"><requirement name="ProgressionLevel" progression_name="attStrength" operation="GTE" value="6" desc_key="reqStrengthLevel06"/></level_requirements>
		<level_requirements level="4"><requirement name="ProgressionLevel" progression_name="attStrength" operation="GTE" value="8" desc_key="reqStrengthLevel08"/></level_requirements>
		<level_requirements level="5"><requirement name="ProgressionLevel" progression_name="attStrength" operation="GTE" value="10" desc_key="reqStrengthLevel10"/></level_requirements>
		
		<effect_group>
			
			<passive_effect name="CraftingTime" operation="perc_subtract" level="1" value=".03"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="2,5" value="1" tags="modDiamondTipGBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="1,5" value="1" tags="modFiberHandleGBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="1,5" value="1" tags="modHandleGBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="1,5" value="1" tags="modCarbideHeadGBAP"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="1,5" value="1" tags="modHeadGBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="1,5" value="1" tags="modStompPlatesGBAP"/>

			<passive_effect name="CraftingTime" operation="perc_subtract" level="2" value=".06"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="2,5" value="1" tags="modGearRedGBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="2,5" value="1" tags="modDiamondScrewGBAP"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="2,5" value="1" tags="modChainBladeGBAP"/>

			<passive_effect name="CraftingTime" operation="perc_subtract" level="3" value=".09"/>	
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="3,5" value="1" tags="modDiamondTipT2GBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="3,5" value="1" tags="modCarbideHeadT2GBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="3,5" value="1" tags="modFiberHandleT2GBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="3,5" value="1" tags="modHandleT2GBAP"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="3,5" value="1" tags="modHeadT2GBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="3,5" value="1" tags="modStompPlatesT2GBAP"/>
			
			<passive_effect name="CraftingTime" operation="perc_subtract" level="4" value=".12"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="2,5" value="1" tags="modEarDefsGBAP"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="2,5" value="1" tags="modAugerBodyGBAP"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="2,5" value="1" tags="modChainBodyGBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="3,5" value="1" tags="modGearRedT2GBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="3,5" value="1" tags="modDiamondScrewT2GBAP"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="3,5" value="1" tags="modChainBladeT2GBAP"/>
			
			<passive_effect name="CraftingTime" operation="perc_subtract" level="5" value=".15"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="4,5" value="1" tags="modDrillTeethGBAP"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="4,5" value="1" tags="modDrillChiselGBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="4,5" value="1" tags="modDrillSocketGBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="4,5" value="1" tags="modDrillSpikeGBAP"/>
            <passive_effect name="RecipeTagUnlocked" operation="base_set" level="4,5" value="1" tags="modMilwackBatteryR3dyGBAP"/>

			<effect_description level="1" desc_key="perkGroundBreakersRank1Desc" long_desc_key="perkGroundBreakersRank1LongDesc"/>
			<effect_description level="2" desc_key="perkGroundBreakersRank2Desc" long_desc_key="perkGroundBreakersRank2LongDesc"/>
			<effect_description level="3" desc_key="perkGroundBreakersRank3Desc" long_desc_key="perkGroundBreakersRank3LongDesc"/>
			<effect_description level="4" desc_key="perkGroundBreakersRank4Desc" long_desc_key="perkGroundBreakersRank4LongDesc"/>
			<effect_description level="5" desc_key="perkGroundBreakersRank5Desc" long_desc_key="perkGroundBreakersRank5LongDesc"/>
	
			<!-- Increase the chance to find Gold Diggers skill magazines with each perk rank up to crafting skill 100 -->
			<passive_effect name="LootProb" operation="perc_add" level="1,5" value="2,10" tags="GoldDiggerCSM">
				<requirement name="ProgressionLevel" progression_name="craftingDiggers" operation="LT" value="100"/>
			</passive_effect>

			<!-- Once Gold Diggers skill reaches 100, set the probability bonus of finding shotgun crafting skill magazines to 0 -->
			<passive_effect name="LootProb" operation="base_set" level="1,5" value="0,0" tags="GoldDiggerCSM">
				<requirement name="ProgressionLevel" progression_name="craftingDiggers" operation="Equals" value="100"/>
			</passive_effect>

			<!-- Increase the chance to find Ground Breakers books until each book is read -->
			<passive_effect name="LootProb" operation="perc_add" level="1,5" value="2,10" tags="GroundBreakersProb">
				<requirement name="ProgressionLevel" progression_name="perkGroundBreakersComplete" operation="LT" value="1"/>
			</passive_effect>

			<!-- When all Ground Breakers books have been read, set the probability bonus of finding them to 0 -->
			<passive_effect name="LootProb" operation="perc_add" level="1,5" value="0,0" tags="GroundBreakersProb">
				<requirement name="ProgressionLevel" progression_name="perkGroundBreakersComplete" operation="Equals" value="1"/>
			</passive_effect>

		</effect_group>
	</perk>
	
