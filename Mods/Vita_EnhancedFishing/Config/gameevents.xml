<snj>
    <append xpath="/gameevents">
        <action_sequence name="action_buffFishingBluegil">
            <action class="BeltTooltip">
                    <property name="text" value="Should I cook you or throw you back." param1="tooltip" />
                    <property name="phase" value="0" />
                </action> 
            <action class="Delay">
                <property name="time" value="15" />
                <property name="phase" value="1" />
            </action>
            <action class="AddItems">
                <property name="added_items" value="foodRawFishBluegil" param1="itemname" />
                <property name="added_item_counts" value="1" param1="itemcount" />
            </action>
            <action class="ModifyCVar">
                <property name="cvar" value="HOH_Fishing_XP" />
                <property name="operation" value="add" />
                <property name="value" value="15" />
	    	</action>
            <action class="PlaySound">
                <property name="sound" value="twitch_good_spawn" />
                <property name="phase" value="1" />
            </action>
        </action_sequence>	
